<%- include('admin/head'); %> <%- include('admin/header'); %>
<div class="container-fluid">
  <div class="card card-primary">
    <div class="card-header">
      <h3 class="card-title">My Profile</h3>
    </div>
  </div>
  <form class="form-horizontal">
    <div class="card-body">
      <div class="form-group">
        <label for="InputVolume">NIK</label>
        <input
          type="number"
          class="form-control"
          id="InputVolume"
          placeholder="Number unit"
          disabled
        />
      </div>
      <div class="form-group">
        <label for="InputVolume">Nama Lengkap</label>
        <input
          type="text"
          class="form-control"
          id="InputVolume"
          placeholder="Number unit"
        />
      </div>
      <div class="form-group">
        <label for="InputVolume">Departeman</label>
        <select class="form-control select2">
          <option selected="selected">Alabama</option>
          <option>Alaska</option>
          <option>California</option>
          <option>Delaware</option>
          <option>Tennessee</option>
          <option>Texas</option>
          <option>Washington</option>
        </select>
      </div>
      <div class="form-group">
        <label for="InputVolume">Jabatan</label>
        <input
          type="text"
          class="form-control"
          id="InputVolume"
          placeholder="Jabatan"
        />
      </div>
      <div class="form-group">
        <label for="InputVolume">Atasan Langsung</label>
        <div class="select2-purple">
          <select class="form-control select2">
            <option selected="selected">Alabama</option>
            <option>Alaska</option>
            <option>California</option>
            <option>Delaware</option>
            <option>Tennessee</option>
            <option>Texas</option>
            <option>Washington</option>
          </select>
        </div>
      </div>
      <div class="card-footer">
        <button type="submit" class="btn btn-primary float-right">Update</button>
      </div>
    </div>
  </form>
<!-- Horizontal Form -->
<div class="card card-info">
    <div class="card-header">
      <h3 class="card-title">My Profile</h3>
    </div>
    <!-- /.card-header -->
    <!-- form start -->
    <form class="form-horizontal">
      <div class="card-body">
        <div class="form-group row">
          <label for="inputEmail3" class="col-sm-2 col-form-label">NIK</label>
          <div class="col-sm-10">
            <input type="number" class="form-control" id="inputEmail3" placeholder="NIK" disabled>
          </div>
        </div>
        <div class="form-group row">
          <label for="inputEmail3" class="col-sm-2 col-form-label">Full Name</label>
          <div class="col-sm-10">
            <input type="text" class="form-control" id="inputEmail3" placeholder="Full Name">
          </div>
        </div>
        <div class="form-group row">
          <label for="inputEmail3" class="col-sm-2 col-form-label">Departeman</label>
          <div class="col-sm-10">
            <select class="form-control select2">
                <option selected="selected">Alabama</option>
                <option>Alaska</option>
                <option>California</option>
                <option>Delaware</option>
                <option>Tennessee</option>
                <option>Texas</option>
                <option>Washington</option>
              </select>
          </div>
        </div>
        <div class="form-group row">
          <label for="inputPassword3" class="col-sm-2 col-form-label">Password</label>
          <div class="col-sm-10">
            <input type="password" class="form-control" id="inputPassword3" placeholder="Password">
          </div>
        </div>
        <div class="form-group row">
          <div class="offset-sm-2 col-sm-10">
            <div class="form-check">
              <input type="checkbox" class="form-check-input" id="exampleCheck2">
              <label class="form-check-label" for="exampleCheck2">Remember me</label>
            </div>
          </div>
        </div>
      </div>
      <!-- /.card-body -->
      <div class="card-footer">
        <button type="submit" class="btn btn-info">Sign in</button>
        <button type="submit" class="btn btn-primary float-right">Update</button>
      </div>
      <!-- /.card-footer -->
    </form>
  </div>
  <!-- /.card -->
</div>
<%- include('admin/footer'); %>
